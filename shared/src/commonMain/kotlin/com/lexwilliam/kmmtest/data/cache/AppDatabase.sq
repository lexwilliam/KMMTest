CREATE TABLE transaction_table (
    id  TEXT NOT NULL,
    name TEXT NOT NULL,
    desc TEXT NOT NULL,
    type TEXT NOT NULL,
    value REAL NOT NULL
);

insertTransaction:
INSERT INTO transaction_table(
    id,
    name,
    desc,
    type,
    value
)
VALUES (?, ?, ?, ?, ?);

removeAllTransactions:
DELETE FROM transaction_table;

selectAllTransactions:
SELECT transaction_table.*
FROM transaction_table;



